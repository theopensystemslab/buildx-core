{
  "name": "lib-buildx",
  "type": "module",
  "private": true,
  "version": "0.0.2",
  "main": "./dist/lib-buildx.cjs",
  "module": "./dist/lib-buildx.mjs",
  "exports": {
    ".": {
      "require": "./dist/lib-buildx.cjs",
      "import": "./dist/lib-buildx.mjs"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite --host",
    "build": "npm run clean && tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint:scripts": "eslint . --ext .ts",
    "lint:styles": "stylelint ./**/*.{css,scss}",
    "format:scripts": "prettier . --write",
    "format:styles": "stylelint ./**/*.{css,scss} --fix",
    "format": "npm run format:scripts && npm run format:styles",
    "prepare": "husky install && husky set .husky/pre-commit 'npx lint-staged' && git add .husky/pre-commit",
    "uninstall-husky": "npm uninstall husky --no-save && git config --unset core.hooksPath && npx rimraf .husky"
  },
  "devDependencies": {
    "@speckle/objectloader": "^2.18.8",
    "@types/dat.gui": "^0.7.12",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.5",
    "@types/three": "^0.161.2",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitest/coverage-v8": "^1.2.1",
    "airtable": "^0.12.2",
    "camera-controls": "^2.8.3",
    "copyfiles": "^2.4.1",
    "cross-fetch": "^4.0.0",
    "dat.gui": "^0.7.9",
    "dts-bundle-generator": "^9.2.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "fp-ts": "^2.16.2",
    "graphql-request": "^6.1.0",
    "husky": "^8.0.3",
    "immer": "^10.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "stylelint": "^16.2.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-sass-guidelines": "^11.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.0",
    "three": "^0.161.0",
    "three-bvh-csg": "^0.0.16",
    "three-mesh-bvh": "^0.7.3",
    "three-stdlib": "^2.29.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.6",
    "vitest": "^1.3.1",
    "zod": "^3.22.4"
  }
}
